<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.twoyum.login.mapper.UserRolesMapper" >

    <insert id="addUserRoles" parameterType="com.twoyum.login.domain.UserRolesInDto">
        insert into t_sys_user_roles(role_id,sys_user_id)
        values(#{roleId},#{sysUserId})
    </insert>

</mapper>